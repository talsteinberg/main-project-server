<div class="container pageContainer">
  <div class="row">
    <div class="col-md-12" >
      <div class="space-down-50">
        <a class="btn btn-primary pull-right" href="/user/create">Add user</a>
      </div>
      <table class="table table-striped table-hover table-bordered noselect">
        <thead>
          <tr class="tableHeader">
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
          </tr>
        </thead>
        <tbody>
        {{#each users}}
          <tr class="table-row" data-id="{{this.id}}">
            <td>{{this.first_name}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.email}}</td>
          </tr>
        {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
<script src="/js/users.list.js"></script>
